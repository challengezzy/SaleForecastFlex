			public function pubFun4setParamIo4periodBeginEnd( _periodBegin:int, _periodEnd:int ):void
			{
				this.paramIo4periodBegin = _periodBegin;
				this.paramIo4periodEnd = _periodEnd;
				
				// 执行两次，强制界面刷新 
				var times:int;
				for( times=0; times<2; times=times+1 )
				{
					var distributedWidth:Number = 0;
					
					if( this.columnIsChecked.visible == true )
					{
						this.columnIsChecked.width = 25;
						distributedWidth = distributedWidth + 25;
					}	
					
					if( this.columnProductCode.visible == true )
					{
						this.columnProductCode.width = 120;
						distributedWidth = distributedWidth + 120;
					}
					if( this.columnProductName.visible == true )
					{
						this.columnProductName.width = 120;
						distributedWidth = distributedWidth + 120;
					}					
					
					if( this.columnOrganizationCode.visible == true )
					{
						this.columnOrganizationCode.width = 120;
						distributedWidth = distributedWidth + 120;
					}
					if( this.columnOrganizationName.visible == true )
					{
						this.columnOrganizationName.width = 120;
						distributedWidth = distributedWidth + 120;
					}					
										
					var totalWidth4DataColumn:Number = this.width - distributedWidth;	
					if( totalWidth4DataColumn < 0 )
					{
						totalWidth4DataColumn = 0;
					}	
					
					var periodDiff:Number = UtilPeriod.getPeriodDifference( this.paramIo4periodBegin, this.paramIo4periodEnd );
					if( periodDiff == SysConst.PERIOD_DIFF_NULL )
					{
						for(var i:int=0; i<this.paramOut4arrDataColumnsStandardPrice.length; i=i+1)
						{
							DataGridColumn( this.paramOut4arrDataColumnsStandardPrice.getItemAt(i) ).visible = false;
							DataGridColumn( this.paramOut4arrDataColumnsRealPrice.getItemAt(i) ).visible = false;
						}
						return;
					}
					
					var widthPerDataCloumn:Number = Math.round( totalWidth4DataColumn/(periodDiff+1) );
					//add by zhangzy 设置成固定宽度65
					//if(widthPerDataCloumn < 60)
					widthPerDataCloumn = 60;
					
					var k:int;
					var dgclm:DataGridColumn = null;
					
					//	两类价格的列宽都设好，是否显示另外控制
					for(k=0; k<=periodDiff && k<this.paramOut4arrDataColumnsStandardPrice.length; k=k+1)
					{
						dgclm = DataGridColumn( this.paramOut4arrDataColumnsStandardPrice.getItemAt(k) );						
						dgclm.headerText = UtilPeriod.getHeaderTextPeriod( this.paramIo4periodBegin, k ); 
						dgclm.width = widthPerDataCloumn;
						if( this.paramIn4visible4StandardPrice == true )
						{
							dgclm.visible = true;
						}
						else
						{
							dgclm.visible = false;
						}
						
					}
					
					for(; k<this.paramOut4arrDataColumnsStandardPrice.length; k=k+1)
					{
						DataGridColumn( this.paramOut4arrDataColumnsStandardPrice.getItemAt(k) ).visible = false; 					
					}						
					
					for(k=0; k<=periodDiff && k<this.paramOut4arrDataColumnsRealPrice.length; k=k+1)
					{
						dgclm = DataGridColumn( this.paramOut4arrDataColumnsRealPrice.getItemAt(k) );
						dgclm.headerText = UtilPeriod.getHeaderTextPeriod( this.paramIo4periodBegin, k ); 
						dgclm.width = widthPerDataCloumn;
						if( this.paramIn4visible4RealPrice == true )
						{
							dgclm.visible = true;
						}
						else
						{
							dgclm.visible = false;
						}						
					}
					
					for(; k<this.paramOut4arrDataColumnsRealPrice.length; k=k+1)
					{
						DataGridColumn( this.paramOut4arrDataColumnsRealPrice.getItemAt(k) ).visible = false; 					
					}							

					
					
					this.validateSize( true );
					this.validateNow();							
				}				
			}	
