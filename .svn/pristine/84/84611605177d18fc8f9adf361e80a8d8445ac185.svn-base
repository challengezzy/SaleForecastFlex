			public function pubFun4setDataColumnEditable( _period:int, _editable:Boolean ):void
			{
				var periodDiff:int = UtilPeriod.getPeriodDifference( periodBegin, periodEnd );	
				if( periodDiff == SysConst.PERIOD_DIFF_NULL  )
				{
					return;	
				}
				
				var periodLoc:int = UtilPeriod.getPeriodDifference( periodBegin, _period );
				if( periodLoc == SysConst.PERIOD_DIFF_NULL  )
				{
					return;	
				}
				
				if( periodLoc <= periodDiff )
				{
					var dgclm:DataGridColumn = DataGridColumn( paramOut4arrDataColumns.getItemAt(periodLoc) );
					dgclm.editable = _editable;		
					if(_editable == true){//更改可编辑字体的样式
						dgclm.setStyle("fontWeight","bold");
						//dgclm.setStyle("backgroundColor","0xF5F7E0");
					}else{
						dgclm.setStyle("fontWeight","normal");
					}
				}
			}
							
			
			public function pubFun4synColumnWidth( _sourceComAdgUiRowData:Com_UiRowData_Report_AdvancedDataGrid ):void
			{
				columnIsChecked.visible 					= _sourceComAdgUiRowData.columnIsChecked.visible;
				columnProductCode.visible 				= _sourceComAdgUiRowData.columnProductCode.visible;
				columnProductName.visible 				= _sourceComAdgUiRowData.columnProductName.visible;
				columnProductCharacterCode.visible 		= _sourceComAdgUiRowData.columnProductCharacterCode.visible;
				columnProductCharacterName.visible 		= _sourceComAdgUiRowData.columnProductCharacterName.visible;				
				columnOrganizationCode.visible 			= _sourceComAdgUiRowData.columnOrganizationCode.visible;
				columnOrganizationName.visible 			= _sourceComAdgUiRowData.columnOrganizationName.visible;
				columnOrganizationCharacterCode.visible 	= _sourceComAdgUiRowData.columnOrganizationCharacterCode.visible;
				columnOrganizationCharacterName.visible 	= _sourceComAdgUiRowData.columnOrganizationCharacterName.visible;				
				columnBizData.visible 					= _sourceComAdgUiRowData.columnBizData.visible;
				
				columnIsChecked.width 					= _sourceComAdgUiRowData.columnIsChecked.width;
				columnProductCode.width 				= _sourceComAdgUiRowData.columnProductCode.width;
				columnProductName.width 				= _sourceComAdgUiRowData.columnProductName.width;
				columnProductCharacterCode.width 		= _sourceComAdgUiRowData.columnProductCharacterCode.width;
				columnProductCharacterName.width 		= _sourceComAdgUiRowData.columnProductCharacterName.width;				
				columnOrganizationCode.width 			= _sourceComAdgUiRowData.columnOrganizationCode.width;
				columnOrganizationName.width 			= _sourceComAdgUiRowData.columnOrganizationName.width;
				columnOrganizationCharacterCode.width 	= _sourceComAdgUiRowData.columnOrganizationCharacterCode.width;
				columnOrganizationCharacterName.width 	= _sourceComAdgUiRowData.columnOrganizationCharacterName.width;				
				columnBizData.width 					= _sourceComAdgUiRowData.columnBizData.width;
				

				var i:int;
				var sourceAdgClm:DataGridColumn = null;
				var targetAdgClm:DataGridColumn = null;
				for( i=0; i<paramOut4arrDataColumns.length; i=i+1 )
				{
					
					sourceAdgClm = DataGridColumn( _sourceComAdgUiRowData.paramOut4arrDataColumns.getItemAt( i ) );
					targetAdgClm = DataGridColumn( paramOut4arrDataColumns.getItemAt( i ) );
					
					if( sourceAdgClm != null && targetAdgClm != null ){
						targetAdgClm.visible = sourceAdgClm.visible;
						targetAdgClm.width = sourceAdgClm.width;
					}
				}
				
				validateNow();
				
			}						
