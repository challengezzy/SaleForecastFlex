package aks.aksas.aidobject
{
	[Bindable]
	public class AksOptionObj
	{
		public function AksOptionObj()
		{
		}
		
		public var name:String ="";
		public var serverName:String = "127.0.0.1";
		public var serverPort:String = "8080";
		public var contextRoot:String = "dmdd";
		public var updateServerName:String = "127.0.0.1";
		public var updateServerPort:String = "8080";
		public var pageSize:int = 20;
		
		public function toString():String
		{
			return this.name;			
		}
	}
}