				var uiPopbScopeBizData:BUiPopbScopeBizData = null;
				for( i=0; i<uiPopbScope.uiPopbScopeBizDatas.length; i=i+1 )
				{
					uiPopbScopeBizData = BUiPopbScopeBizData( uiPopbScope.uiPopbScopeBizDatas.getItemAt(i) );
					
					prvVar4dp4comAdgBizData.addItem( uiPopbScopeBizData.bizData );
				}
				//	BizData	end
				
				//	显示控制		begin
				if( uiPopbScope.isDisplayControl == BizConst.GLOBAL_YESNO_YES )
				{
					isDisplayProduct = uiPopbScope.isShowProduct == BizConst.GLOBAL_YESNO_YES ?true:false;
					isDisplayProductCharacter = uiPopbScope.isShowProductCharacter == BizConst.GLOBAL_YESNO_YES ?true :false;
					isDisplayOrganization = uiPopbScope.isShowOrganization == BizConst.GLOBAL_YESNO_YES ?true :false;
					isDisplayOrganizationCharacter = uiPopbScope.isShowOrganizationCharacter == BizConst.GLOBAL_YESNO_YES ?true :false;
					
