			public function pubFun4setParamIo4periodStatistics( _periodBegin:int, _periodEnd:int, _editable:Boolean ,_period:int):void
			{
				this.paramIo4periodBegin = _periodBegin;
				this.paramIo4periodEnd = _periodEnd;
				
				// 执行两次，强制界面刷新 
				var times:int;
				for( times=0; times<2; times=times+1 )
				{
					var distributedWidth:Number = 0;
					
					if( this.columnIsChecked.visible == true )
					{
						this.columnIsChecked.width = 50;
						distributedWidth = distributedWidth + 50;
					}	
					
					if( this.columnProductCode.visible == true )
					{
						this.columnProductCode.width = 100;
						distributedWidth = distributedWidth + 100;
					}
					if( this.columnProductName.visible == true )
					{
						this.columnProductName.width = 100;
						distributedWidth = distributedWidth + 100;
					}					
					if( this.columnProductCharacterCode.visible == true )
					{
						this.columnProductCharacterCode.width = 100;
						distributedWidth = distributedWidth + 100;
					}
					if( this.columnProductCharacterName.visible == true )
					{
						this.columnProductCharacterName.width = 100;
						distributedWidth = distributedWidth + 100;
					}										
					
					if( this.columnOrganizationCode.visible == true )
					{
						this.columnOrganizationCode.width = 100;
						distributedWidth = distributedWidth + 100;
					}
					if( this.columnOrganizationName.visible == true )
					{
						this.columnOrganizationName.width = 100;
						distributedWidth = distributedWidth + 100;
					}					
					if( this.columnOrganizationCharacterCode.visible == true )
					{
						this.columnOrganizationCharacterCode.width = 100;
						distributedWidth = distributedWidth + 100;
					}
					if( this.columnOrganizationCharacterName.visible == true )
					{
						this.columnOrganizationCharacterName.width = 100;
						distributedWidth = distributedWidth + 100;
					}	
					
					this.columnBizData.width = 100;
					distributedWidth = distributedWidth + 100;
					
					var totalWidth4DataColumn:Number = this.width - distributedWidth;	
					if( totalWidth4DataColumn < 0 )
					{
						totalWidth4DataColumn = 0;
					}	
					
					var periodDiff:Number = UtilPeriod.getPeriodDifference( this.paramIo4periodBegin, this.paramIo4periodEnd );
					var diff:int = (periodDiff+1)/_period;
					if( periodDiff == SysConst.PERIOD_DIFF_NULL )
					{
						for(var i:int=0; i<this.paramOut4arrDataColumns.length; i=i+1)
						{
							DataGridColumn( this.paramOut4arrDataColumns.getItemAt(i) ).visible = false;
						}
						return;
					}
					
					var widthPerDataCloumn:Number = Math.round( totalWidth4DataColumn/(diff+2) );
					
					var k:int;
					var dgclm:DataGridColumn = null;
					for(k=0; k<diff && k<this.paramOut4arrDataColumns.length; k=k+1)
					{
						dgclm = DataGridColumn( this.paramOut4arrDataColumns.getItemAt(k) );
						dgclm.visible = true;
						dgclm.headerText = UtilPeriod.getPeriod( this.paramIo4periodBegin, k*_period )+"-"+UtilPeriod.getPeriod( this.paramIo4periodBegin, (k+1)*_period-1 ); 
						dgclm.editable = _editable;
						dgclm.width = widthPerDataCloumn;						
					}
					//增加个汇总列
					
					dgclm = DataGridColumn( this.paramOut4arrDataColumns.getItemAt(k) );
					dgclm.visible = true;
					dgclm.headerText = ResourceManager.getInstance().getString('label','total'); 
					dgclm.editable = _editable;
					dgclm.width = widthPerDataCloumn;
					k=k+1
					for(; k<this.paramOut4arrDataColumns.length; k=k+1)
					{
						DataGridColumn( this.paramOut4arrDataColumns.getItemAt(k) ).visible = false; 					
					}						
					
					this.validateSize( true );
					this.validateNow();							
				}
			}
			
			public function pubFun4setParamIo4periodBreakdown( _periodBegin:int, _periodEnd:int, _editable:Boolean ,_hm4BreakDownRuleDefItem:HashMap):void
			{
				this.paramIo4periodBegin = _periodBegin;
				this.paramIo4periodEnd = _periodEnd;
				
				// 执行两次，强制界面刷新 
				var times:int;
				for( times=0; times<2; times=times+1 )
				{
					var distributedWidth:Number = 0;
					
					if( this.columnIsChecked.visible == true )
