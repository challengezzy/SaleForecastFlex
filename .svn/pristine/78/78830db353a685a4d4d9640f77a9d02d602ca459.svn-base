			/** 列字段显示设置
			 * @param _refAdg 需要同步的Adg
			 * */
			public function onColumnSet():void
			{
				if (columnSetWindow == null){
					columnSetWindow = new Com_UiRowData_ColumnSet();
					columnSetWindow.onOkFunction = onColumnSetOk;
				}
				PopUpManager.addPopUp(columnSetWindow,this);
				PopUpManager.centerPopUp(columnSetWindow);
				
				//设置初始状态
				columnSetWindow.isProCode = showProCode;
				columnSetWindow.isProName = showProName;
				columnSetWindow.isProCharCode = showProCharCode;
				columnSetWindow.isProCharName = showProCharName;
				columnSetWindow.isOrgCode = showOrgCode;
				columnSetWindow.isOrgName = showOrgName;
				columnSetWindow.isOrgCharCode = showOrgCharCode;
				columnSetWindow.isOrgCharName = showOrgCharName;
				
				columnSetWindow.checkProCode.enabled = visibleProduct;
				columnSetWindow.checkProName.enabled = visibleProduct;
				columnSetWindow.checkProCharCode.enabled = visibleProductChar;
				columnSetWindow.checkProCharName.enabled = visibleProductChar;
				columnSetWindow.checkOrgCode.enabled = visibleOrganization;
				columnSetWindow.checkOrgName.enabled = visibleOrganization;
				columnSetWindow.checkOrgCharCode.enabled = visibleOrganizationChar;
				columnSetWindow.checkOrgCharName.enabled = visibleOrganizationChar;
				
			}
			
			private function onColumnSetOk():void{
				//更新显示列设置
				showProCode = columnSetWindow.isProCode;
				showProName = columnSetWindow.isProName;
				showProCharCode = columnSetWindow.isProCharCode;
				showProCharName = columnSetWindow.isProCharName;
				showOrgCode = columnSetWindow.isOrgCode;
				showOrgName = columnSetWindow.isOrgName;
				showOrgCharCode = columnSetWindow.isOrgCharCode;
				showOrgCharName = columnSetWindow.isOrgCharName;
				
			}
			
