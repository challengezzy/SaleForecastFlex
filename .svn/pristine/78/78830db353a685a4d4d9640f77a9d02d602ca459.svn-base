						this.columnProductCharacterCode.width = 100;
						distributedWidth = distributedWidth + 100;
					}
					if( this.columnProductCharacterName.visible == true )
					{
						this.columnProductCharacterName.width = 100;
						distributedWidth = distributedWidth + 100;
					}										
					
					if( this.columnOrganizationCode.visible == true )
					{
						this.columnOrganizationCode.width = 100;
						distributedWidth = distributedWidth + 100;
					}
					if( this.columnOrganizationName.visible == true )
					{
						this.columnOrganizationName.width = 100;
						distributedWidth = distributedWidth + 100;
					}					
					if( this.columnOrganizationCharacterCode.visible == true )
					{
						this.columnOrganizationCharacterCode.width = 100;
						distributedWidth = distributedWidth + 100;
					}
					if( this.columnOrganizationCharacterName.visible == true )
					{
						this.columnOrganizationCharacterName.width = 100;
						distributedWidth = distributedWidth + 100;
					}	
					
					this.columnBizData.width = 100;
					distributedWidth = distributedWidth + 100;
					
					var totalWidth4DataColumn:Number = this.width - distributedWidth;	
					if( totalWidth4DataColumn < 0 )
					{
						totalWidth4DataColumn = 0;
					}	
					var periodDiff:Number =breakdownfinacilDefItems.length;		
					if( periodDiff == SysConst.PERIOD_DIFF_NULL )
					{
						for(var i:int=0; i<this.paramOut4arrDataColumns.length; i=i+1)
						{
							DataGridColumn( this.paramOut4arrDataColumns.getItemAt(i) ).visible = false;
						}
						return;
					}
					
					var widthPerDataCloumn:Number = Math.round( totalWidth4DataColumn/(periodDiff*5+2) );
					
					var k:int=0;
					var dgclm:DataGridColumn = null;
					var num_week:int = 0;
					//					var arrPeriods:ArrayCollection = new ArrayCollection(_periodSet.getKeys());
					//					//将期间排序
					//					var numericDataSort:Sort = new Sort(); 
					//					numericDataSort.fields = [new SortField(null, true)];
					//					arrPeriods.sort = numericDataSort;
					//					arrPeriods.refresh();
					var periodLoc_:int = 0;
					var period:int;
					var periodDiff_:int = UtilPeriod.getPeriodDifference( _periodBegin,_periodEnd );
					for( periodLoc_ = 0; periodLoc_ <= periodDiff_; periodLoc_ = periodLoc_ + 1 )
					{
						
						period= UtilPeriod.getPeriod(_periodBegin,periodLoc_);
						var istrue:Boolean = false;
						var breakDownRuleItem:BBreakDownRuleFinancialDefItem ;
						for(var m:int = 0;m<periodDiff;m++)
						{	
							breakDownRuleItem = BBreakDownRuleFinancialDefItem(breakdownfinacilDefItems.getItemAt(m));
							if(period == breakDownRuleItem.period)
							{
								if(k>79)
								{
									return ;											
								}
								dgclm = DataGridColumn( this.paramOut4arrDataColumns.getItemAt(k) );
								dgclm.visible = true;
								dgclm.headerText = breakDownRuleItem.weekCode; 
								dgclm.editable = _editable;
								dgclm.width = 50;		
								k++;
								istrue = true;
								
							}
							else if(istrue==true)
							{
								break;
							}
						}
					}
					for(; k<this.paramOut4arrDataColumns.length; k=k+1)
					{
						DataGridColumn( this.paramOut4arrDataColumns.getItemAt(k) ).visible = false; 					
					}						
					
					this.validateSize( true );
					this.validateNow();							
				}
			}
			
			public function pubFun4setDataColumnEditable( _period:int, _editable:Boolean ):void
			{
				var periodDiff:int = UtilPeriod.getPeriodDifference( this.paramIo4periodBegin, this.paramIo4periodEnd );	
				if( periodDiff == SysConst.PERIOD_DIFF_NULL  )
				{
					return;	
				}
				
				var periodLoc:int = UtilPeriod.getPeriodDifference( this.paramIo4periodBegin, _period );
				if( periodLoc == SysConst.PERIOD_DIFF_NULL  )
				{
					return;	
				}
				
				if( periodLoc <= periodDiff )
				{
					var dgclm:DataGridColumn = DataGridColumn( this.paramOut4arrDataColumns.getItemAt(periodLoc) );
					dgclm.editable = _editable;					
				}
			}
			
			
			
